
<h1>Updater#set_rows</h1>


<%= link_to 'Back', updater_index_path, class: "btn btn-primary" %>
<button id="but">Skip rows</button>
<hr>

<%= render 'flash'  %>

<%= render 'header'  %>

<hr>
<table id="table" class="display" cellspacing="0" width="100%"  >
  <thead>
    <tr>
      <% @cols.each do |r| %>
      <th><%= r %></th>
      <% end %>
    </tr>
  </thead>
  
  <tfoot>
    <tr>
      <% @cols.each do |r| %>
      <th><%= r %></th>
      <% end %>
    </tr>
  </tfoot>

  <tbody>
    <% @data.each do |e| %>
    <tr>
      <% @cols.each do |r| %>
      <td>
	<%= e.send(r).to_s[0..25] %>
      </td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>

<script><%= render 'script'  %></script>
